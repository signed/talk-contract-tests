{
    "provider": {
        "name": "CalculatorService"
    },
    "consumer": {
        "name": "OperationsService"
    },
    "interactions": [
        {
            "description": "power down calculator",
            "request": {
                "method": "DELETE",
                "path": "/calculator/power"
            },
            "response": {
                "status": 200,
                "body": {
                    "status": "off"
                }
            },
            "providerStates": [
                {
                    "name": "calculator online"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.14"
        }
    }
}